/*
 * search-list
 */
@import "../variables.styl"
@import "../mixins.styl"

.svz-search-list
  border-box()
  margin-top: 40px

.svz-search-list__header
  margin-bottom: 25px

.svz-search-list__search
  display block
  border 1px solid #bfbfbf
  border-radius default-border-radius
  height 40px
  line-height 40px
  padding 2px